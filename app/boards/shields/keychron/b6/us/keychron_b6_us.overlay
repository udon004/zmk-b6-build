        RC(1,1) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(1,4) RC(1,5) RC(0,5) RC(0,6) RC(0,7)
        RC(0,8) RC(1,8) RC(1,6) RC(2,10) RC(6,11) RC(7,14) RC(7,13) RC(0,11) RC(0,12) RC(0,13) RC(0,14)
